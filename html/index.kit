			<section class="offer section-padding section" id="offer">
				<h2 class="offer__title section-title">oferta</h2>
				<p class="offer__caption">Sprawdź polecane przez nas urządzenia</p>
				<div class="wrapper offer__cards">
					<div class="offer__card offer__card__one">
						<p class="offer__card__title offer__card__item">Demo</p>
						<p class="offer__card__price offer__card__item">$0</p>
						<ul class="offer__card__list offer__card__item">
							<li class="offer__card__list-item">
								<i class="fa-solid fa-check"></i>opcja w pakiecie
							</li>
							<li class="offer__card__list-item">
								<i class="fa-solid fa-check"></i>opcja w pakiecie
							</li>
							<li
								class="offer__card__list-item offer__card__list-item--inactive">
								<i class="fa-solid fa-minus"></i>opcja poza pakietem
							</li>
							<li
								class="offer__card__list-item offer__card__list-item--inactive">
								<i class="fa-solid fa-minus"></i>opcja poza pakietem
							</li>
							<li
								class="offer__card__list-item offer__card__list-item--inactive">
